{
    "name": "blockchain-test",
    "version": "1.0.0",
    "main": "index.ts",
    "scripts": {
        "checkSolc": "sudo apt-get install -y snapd | sudo snap install solc",
        "dev": "concurrently --kill-others \"npm run remix\" \"npm run ganache\" \"npm run serve\"",
        "dev_nr": "concurrently --kill-others \"npm run ganache\" \"npm run serve\"",
        "dev0": "concurrently --kill-others \"npm run remix\" \"npm run state\" \"npm run serve\"",
        "dev1": "concurrently --kill-others \"npm run remix\" \"npm run serve\"",
        "ganache": "ganache-cli --account_keys_path keys.json -a 100 -m 'viable badge wasp define ten slim spider wolf slice great unaware neck'",
        "remix": "remixd -s ./contracts --remix-ide https://remix.ethereum.org",
        "state": "ganache-cli --account_keys_path keys.json -a 100 --db='./state/' -i='1337' --mnemonic='viable badge wasp define ten slim spider wolf slice great unaware neck'",
        "serve": "nodemon ./index.ts"
    },
